{
  "name": "ace-dms",
  "version": "1.0.0",
  "description": "a document management system",
  "main": "server.js",
  "scripts": {
    "start:server": "pm2 start server/server.js --interpreter babel-node --watch",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "test": "npm run test:client && npm run test:server",
    "test-e2e": "npm run undo:migrations && npm run migrations && npm run seeds && node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "undo:migrations": "sequelize db:migrate:undo:all",
    "mocha": "mocha --compilers js:babel-core/register",
    "start:client": "webpack-dev-server --progress --inline --open --watch",
    "build": "rimraf dist && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "migrations": "sequelize db:migrate",
    "seeds": "sequelize db:seed:all",
    "undo:seeds": "sequelize db:seed:undo:all",
    "test:server": "NODE_ENV=test npm run mocha server/tests/**/*.spec.js",
    "test:client": "mocha client/tests/helpers/browser.js client/tests/**/*.spec.js client/tests/**/**/*.spec.js --compilers js:babel-register",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./",
    "lint": "node_modules/.bin/eslint . || true",
    "lint:watch": "npm run lint -- --watch",
    "cover": "rm -rf coverage/ && NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover _mocha -- -R spec server/tests/**/*.spec.js",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "postinstall": "npm run migrations && npm run undo:seeds && npm run seeds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-rhenshaw/Ace-DMS.git"
  },
  "keywords": [
    "document",
    "management",
    "system",
    "ace-dms"
  ],
  "author": "Rowland Henshaw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-rhenshaw/Ace-DMS/issues"
  },
  "homepage": "https://github.com/andela-rhenshaw/Ace-DMS#readme",
