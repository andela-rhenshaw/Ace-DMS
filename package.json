{
  "name": "ace-dms",
  "version": "1.0.0",
  "description": "a document management system",
  "main": "server.js",
  "scripts": {
    "mocha": "mocha --compilers js:babel-core/register",
    "test": "NODE_ENV=test npm run mocha server/test",
    "lint": "node_modules/.bin/eslint . || true",
    "lint:watch": "npm run lint -- --watch",
    "cover": "rm -rf coverage/ && NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec server/test",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-rhenshaw/Ace-DMS.git"
  },
  "keywords": [
    "document",
    "management",
    "system",
    "ace-dms"
  ],
  "author": "Rowland Henshaw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-rhenshaw/Ace-DMS/issues"
  },
  "homepage": "https://github.com/andela-rhenshaw/Ace-DMS#readme",
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "webpack": "^2.3.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.1",
    "coveralls": "^2.11.15",
    "eslint": "^3.14.0",
    "eslint-plugin-react": "^6.9.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "morgan": "^1.8.1",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
